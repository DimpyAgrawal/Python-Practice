import numpy as np
r,c=list(map(int,input().split()))
a=list(map(int,input().split()))
b=list(map(int,input().split()))
ad=np.add(a,b)
sb=np.subtract(a,b)
mul=np.multiply(a,b)
div=np.divide(a,b)
modulus=np.mod(a,b)
p=np.power(a,b)
print(ad)                   
print(sb)
print(mul)
print(div)
print(modulus)
print(p)
